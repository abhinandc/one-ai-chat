{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": false,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "src/index.css",
    "baseColor": "slate",
    "cssVariables": true,
    "prefix": ""
  },
  "iconLibrary": "lucide",
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
  "registries": {
    "@blocks": "https://blocks.so/r/{name}.json",
    "@cult-ui": "https://cult-ui.com/r/{name}.json",
    "@cult-ui-pro": {
      "url": "https://pro.cult-ui.com/api/registry/{name}",
      "headers": {
        "X-API-Key": "${CULT_PRO_TOKEN}"
      }
    },
    "@animate-ui": "https://animate-ui.com/r/{name}.json",
    "@prompt-kit": "https://prompt-kit.com/c/{name}.json",
    "@ai-elements": "https://ai-sdk.dev/elements/api/registry/{name}.json",
    "@diceui": "https://diceui.com/r/{name}.json"
  }
}
